            </generator>
        </id>

        <property
            name="cmptVolume"
            type="java.lang.String"
            update="true"
            insert="true"
            column="cmptVolume"
        />

        <property
            name="custTarget"
            type="java.lang.String"
            update="true"
            insert="true"
            column="custTarget"
