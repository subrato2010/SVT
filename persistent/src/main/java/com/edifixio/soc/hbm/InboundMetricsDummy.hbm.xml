            type="java.lang.String"
            update="true"
            insert="true"
            column="metricsName"
        />

        <property
            name="percentIncrease"
            type="java.lang.String"
            update="true"
            insert="true"
            column="percentIncrease"
        />

        <property
            name="profileId"
            type="java.lang.String"
            update="true"
            insert="true"
            column="profileId"
        />
		<property
            name="targetModerate"
            type="java.lang.String"
            update="true"
            insert="true"
            column="targetModerate"
        />
        <property
            name="targetAggressive"
